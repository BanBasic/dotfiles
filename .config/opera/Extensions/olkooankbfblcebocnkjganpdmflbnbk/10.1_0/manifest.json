{
   "author": "Yuri Konotopov \u003Cykonotopov@gnome.org>",
   "background": {
      "page": "extension.html",
      "persistent": false
   },
   "browser_action": {
      "default_icon": {
         "16": "icons/GnomeLogo-16.png",
         "32": "icons/GnomeLogo-32.png"
      },
      "default_title": "__MSG_open_website__"
   },
   "content_scripts": [ {
      "js": [ "include/constants.js", "include/compat-content-script.js", "content-script-start.js" ],
      "matches": [ "https://extensions.gnome.org/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_manifest_description__",
   "developer": {
      "name": "ne0sight"
   },
   "externally_connectable": {
      "matches": [ "https://extensions.gnome.org/*" ]
   },
   "icons": {
      "128": "icons/GnomeLogo-128.png",
      "16": "icons/GnomeLogo-16.png",
      "48": "icons/GnomeLogo-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_gs_chrome__",
   "optional_permissions": [ "idle" ],
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "alarms", "nativeMessaging", "notifications", "storage", "https://extensions.gnome.org/" ],
   "short_name": "GS Chrome",
   "update_url": "https://extension-updates.opera.com/api/omaha/update/",
   "version": "10.1",
   "web_accessible_resources": [ "include/sweettooth-api.js" ]
}
